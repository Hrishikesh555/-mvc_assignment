@model EmployeProj.Models.Department
@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayoutPage1.cshtml";
}

<h2>Details</h2>
<table class="table">
    <tr class="table-light">
        <th>Department Id</th>
        <td>@Model.DepartmentId</td>
    </tr>
    <tr class="table-primary">
        <th>Department Name</th>
        <td>@Model.DepartmentName</td>
    </tr>
    <tr>
        <th>Department Location</th>
        <td>@Model.DepartmentLoc</td>
    </tr>
</table>

<a class="btn btn-outline-primary" href="/Admin/Department/Index">Back</a>